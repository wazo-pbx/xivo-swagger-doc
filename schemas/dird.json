    ],
    "basePath": "/0.1",
    "consumes": [
        "application/json"
    ],
    "produces": [
        "application/json"
    ],
    "paths": {
        "/directories/lookup/{profile}": {
            "get": {
                "summary": "Lookup results",
                "description": "Get a list contact results for directories in profile",
                "operationId": "get_profile",
                "responses": {
                    "200": {
                        "description": "A list of contacts",
                        "schema": {
                            "$ref": "#/definitions/Lookup"
                        }
                    },
                    "404": {
                        "description": "Invalid lookup",
                        "schema": {
                            "$ref": "#/definitions/Error"
                        }
                    }
                },
                "parameters": [
                    {
                        "name": "profile",
                        "in": "path",
                        "type": "string",
                        "description": "The profile identifies the list of contact sources and the display format",
                        "required": true
                    },
                    {
                        "name": "term",
                        "in": "query",
                        "type": "string",
                        "description": "The string to look for",
                        "required": true
                    }
                ]
            }
        },
        "/directories/lookup/{profile}/headers": {
            "get": {
                "summary": "Lookup headers",
                "description": "Headers that should be displayed when displaying results of a lookup",
                "operationId": "get_headers",
                "responses": {
                    "200": {
                        "description": "A list of headers",
                        "schema": {
                            "$ref": "#/definitions/Header"
                        }
                    },
                    "404": {
                        "description": "Invalid lookup",
                        "schema": {
                            "$ref": "#/definitions/Error"
                        }
                    }
                },
                "parameters": [
                    {
                        "name": "profile",
                        "in": "path",
                        "type": "string",
                        "description": "The profile identifies the list of contact sources and the display format",
                        "required": true
                    }
                ]
            }
        }
    },
    "definitions": {
        "Error": {
            "title": "Error",
            "description": "Error message for the client",
            "properties": {
                "timestamp": {
                    "description": "Time at which the error occured",
                    "type": "array",
                    "items": {
                        "type": "number",
                        "format": "timestamp"
                    }
                },
                "reason": {
                    "description": "Human readable explanation of the error",
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "status_code": {
                    "description": "HTTP status code",
                    "type": "integer"
                }
            },
            "title": "Invalid profile",
            "description": "Profile does not exist",
            "type": "string"
        },
        "Header": {
            "properties": {
                "column_types": {
                    "type": "array",
                    "description": "The types for the result header",
                    "items": {
                        "type": "string"
                    }
                },
                "column_headers": {
                    "type": "array",
                    "description": "The labels for the result header",
                    "items": {
                        "type": "string"
                    }
                }
            }
        },
        "Lookup": {
            "properties": {
                "column_types": {
                    "items": {
                        "type": "string"
                    },
                    "type": "array",
                    "description": "The types of the result header"
                },
                "term": {
                    "type": "string",
                    "description": "The string to look for"
                },
                "column_headers": {
                    "items": {
                        "type": "string"
                    },
                    "type": "array",
                    "description": "The labels of the result header"
                },
                "results": {
                    "items": {
                        "items": {
                            "type": "string"
                        },
                        "type": "array"
                    },
                    "type": "array",
                    "description": "The values of the results"
                }
            }
        }
    }
